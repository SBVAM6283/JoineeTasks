{
  "name": "appdevstudio",
  "version": "1.0.0",
  "author": "valuemomentum",
  "scripts": {
    "start:dev": "npm-run-all -p electron-build electron:serve ng:serve",
    "start": "tsc ./src/electron.dev.ts && ng build --base-href ./ && electron ./src/electron.dev",
    "package:win": "npm run build_prod  && npx electron-packager prod_dist appdevstudio --out=packages --platform=win32 --overwrite ",
    "package:linux": "npm run ng build --prod --base-href ./ --output-path=prod_dist --source-map=false && cross-var electron-packager prod_dist $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=all --overwrite ",
    "package:osx": "npm run ng build --prod --base-href ./ --output-path=prod_dist --source-map=false && cross-var electron-packager prod_dist $npm_package_name-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite ",
    "package:all": "npm run ng build --prod --base-href ./ --output-path=prod_dist --source-map=false && cross-var electron-packager prod_dist $npm_package_name-$npm_package_version --out=packages --all --arch=all --overwrite ",
    "ng:serve": "ng serve",
    "ng": "ng",
    "build": "tsc ./src/electron.dev.ts && ng build --base-href ./",
   "build_dev": "tsc ./src/electron.dev.ts && ng build --output-path=dev_dist --base-href ./  --source-map=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "setup": "node build_installer.js",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc ",
    "electron:serve-tsc": "tsc ./src/electron.dev.ts",
    "electron-build": "ng build --base-href ./ && npx electron ./src/electron.dev --serve",
    "electron-package:dev": "npm run build_dev&& npm install && npx electron-packager dev_dist app_dev --out=packages --prune=false --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite",
    "electron-package:test": "npm run build_dev && npx electron-packager dev_dist app_test --out=packages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^11.2.10",
    "@angular/common": "~11.0.3",
    "@angular/compiler": "~11.0.3",
    "@angular/core": "~11.0.3",
    "@angular/forms": "~11.0.3",
    "@angular/localize": "^11.2.12",
    "@angular/platform-browser": "~11.0.3",
    "@angular/platform-browser-dynamic": "~11.0.3",
    "@angular/router": "~11.0.3",
    "@circlon/angular-tree-component": "^11.0.4",
    "@ng-bootstrap/ng-bootstrap": "^9.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@swimlane/dragula": "^3.8.0",
    "@swimlane/ngx-dnd": "^9.0.0",
    "@types/async": "^3.2.6",
    "@types/dragula": "^3.7.0",
    "bdstudio": "file:prod_dist",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "class-transformer": "^0.4.0",
    "core-js": "^3.18.0",
    "electron-log": "^4.3.5",
    "electron-reload": "^1.5.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "get-port": "^5.1.1",
    "jquery": "^3.6.0",
    "jsondiffpatch": "^0.4.1",
    "mxgraph": "^4.2.2",
    "ng-bootstrap": "^1.6.3",
    "ng2-search-filter": "^0.5.1",
    "ngx-contextmenu": "^5.4.0",
    "ngx-malihu-scrollbar": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "primeflex": "2.0.0",
    "primeicons": "^4.1.0",
    "primeng": "^11.4.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.0",
    "stream": "0.0.2",
    "terser": "3.14.1",
    "timers": "^0.1.1",
    "tslib": "^2.2.0",
    "update-electron-app": "^2.0.1",
    "uuid": "^8.3.2",
    "xml2js": "^0.4.23",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.7",
    "@angular/cli": "~11.0.3",
    "@angular/compiler-cli": "~11.0.3",
    "@azure/service-bus": "^7.3.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.16",
    "@types/uuid": "^8.3.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^6.0.2",
    "cross-var": "^1.1.0",
    "electron": "^11.4.3",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.2.0",
    "electron-winstaller": "^5.0.0",
    "electron-wix-msi": "^3.2.0",
    "exe-icon-extractor": "^1.0.8",
    "install": "^0.13.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "wait-on": "^5.3.0"
  }
}
